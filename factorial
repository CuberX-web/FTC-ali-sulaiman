class HelloWorld {
    public static void main(String[] args) {
        
       int x = factorial(5);
       System.out.println(x);
        
        
    }
    
        public static int factorial(int number) 
        {
        int result = 1;
        
       for(int factorial = 2; factorial <= number; factorial++)
       {
           
           result * = factorial;
       }
  }
}
