class HelloWorld {
    public static void main(String[] args) {
       
         System.out.println(sumFromTo(4 , 7));
        System.out.println(sumFromTo(-3 , 1));
        
        System.out.println(sumFromTo2(7 , 4));
        System.out.println(sumFromTo(9 , 9));
        
    }
    public static int sumFromTo(int start, int end){
    int m = 0;
    
     for (int i = start; i <= end; i++) {
      m = m + i;
    
        }
    return m;
      }
    
    public static int sumFromTo2(int startt, int endd){
    int x = 0;
    
    for (int i = endd; i <= startt; i++) {
      x = x + i;
    
        }
        return x;
    }
}
